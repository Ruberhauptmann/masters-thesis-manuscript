    \entry{yuanSupercurrentDiodeEffect2022}{article}{}
      \name{author}{2}{}{%
        {{hash=8c5f4573806aaf1db65ba6dbbece96d7}{%
           family={Yuan},
           familyi={Y\bibinitperiod},
           given={Noah\bibnamedelimb F.\bibnamedelimi Q.},
           giveni={N\bibinitperiod\bibinitdelim F\bibinitperiod\bibinitdelim Q\bibinitperiod}}}%
        {{hash=159de599cf141a3147da71ab6973e85c}{%
