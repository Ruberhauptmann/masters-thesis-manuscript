      \strng{namehash}{d337245e299a90e4c87c62bbde6ccf56}
      \strng{fullhash}{d337245e299a90e4c87c62bbde6ccf56}
      \strng{bibnamehash}{d337245e299a90e4c87c62bbde6ccf56}
