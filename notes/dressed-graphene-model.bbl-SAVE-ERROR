      \strng{namehash}{e103d9fa7250cb8b798943aa19d74749}
      \strng{fullhash}{e103d9fa7250cb8b798943aa19d74749}
      \strng{bibnamehash}{e103d9fa7250cb8b798943aa19d74749}
      \strng{authorbibnamehash}{e103d9fa7250cb8b798943aa19d74749}
