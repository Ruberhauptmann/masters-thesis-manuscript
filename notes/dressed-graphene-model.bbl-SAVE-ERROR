    \entry{nuckollsMicroscopicPerspectiveMoire2024}{article}{}
      \name{author}{2}{}{%
        {{hash=9deb450dae476cec60601bddfed098a9}{%
           family={Nuckolls},
           familyi={N\bibinitperiod},
           given={Kevin\bibnamedelima P.},
           giveni={K\bibinitperiod\bibinitdelim P\bibinitperiod}}}%
        {{hash=cebfde270e2b1175f3606926461c2a19}{%
           family={Yazdani},
           familyi={Y\bibinitperiod},
           given={Ali},
           giveni={A\bibinitperiod}}}%
