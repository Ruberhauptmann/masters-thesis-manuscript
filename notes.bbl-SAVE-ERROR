      }
      \strng{namehash}{f56d851647406a5436d864fe423cc9d5}
      \strng{fullhash}{f56d851647406a5436d864fe423cc9d5}
      \strng{bibnamehash}{f56d851647406a5436d864fe423cc9d5}
      \strng{authorbibnamehash}{f56d851647406a5436d864fe423cc9d5}
      \strng{authornamehash}{f56d851647406a5436d864fe423cc9d5}
