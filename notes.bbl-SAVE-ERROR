    \entry{marzariMaximallyLocalizedWannier2012}{article}{}
      \name{author}{5}{}{%
        {{hash=789148b9644893e2b07e550eec09d89c}{%
           family={Marzari},
           familyi={M\bibinitperiod},
           given={Nicola},
           giveni={N\bibinitperiod}}}%
        {{hash=082ec900b9a7eeb23474b62c689e115c}{%
           family={Mostofi},
