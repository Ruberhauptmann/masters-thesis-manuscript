    \entry{giaeverStudySuperconductorsElectron1961}{article}{}
      \name{author}{2}{}{%
        {{hash=38d55ac52e8a0be36c5581b5082c17ee}{%
           family={Giaever},
           familyi={G\bibinitperiod},
           given={Ivar},
           giveni={I\bibinitperiod}}}%
        {{hash=5624649245171885bfe6f42c09299bd3}{%
           family={Megerle},
           familyi={M\bibinitperiod},
           given={Karl},
