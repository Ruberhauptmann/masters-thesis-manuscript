    \entry{wehlingDiracMaterials2014}{article}{}
      \name{author}{3}{}{%
        {{hash=3d48715bea79b058bb11974ec6257e15}{%
           family={Wehling},
           familyi={W\bibinitperiod},
           given={T.O.},
