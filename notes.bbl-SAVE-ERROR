      \strng{bibnamehash}{b6076998726c2e9bf36e79d735c1abff}
      \strng{authorbibnamehash}{b6076998726c2e9bf36e79d735c1abff}
      \strng{authornamehash}{b6076998726c2e9bf36e79d735c1abff}
