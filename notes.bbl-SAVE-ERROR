    \entry{bachParasweepTemplatebasedUtility2021}{article}{}
      \name{author}{1}{}{%
        {{hash=68d4a6e087f6dfe738ef59c09683695c}{%
           family={Bach},
           familyi={B\bibinitperiod},
           given={Eviatar},
