        {{hash=7a344a84e0315d10d2e126ff77663643}{%
           family={Black-Schaffer},
           familyi={B\bibinithyphendelim S\bibinitperiod},
           given={A.M.},
           giveni={A\bibinitperiod}}}%
        {{hash=d5547841177967dc7b245505f264e1a8}{%
           family={Balatsky},
           familyi={B\bibinitperiod},
           given={A.V.},
           giveni={A\bibinitperiod}}}%
