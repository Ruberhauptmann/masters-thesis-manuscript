      \strng{namehash}{da3293d65a0cde73cd3f3441689a49b0}
      \strng{fullhash}{da3293d65a0cde73cd3f3441689a49b0}
      \strng{bibnamehash}{da3293d65a0cde73cd3f3441689a49b0}
      \strng{authorbibnamehash}{da3293d65a0cde73cd3f3441689a49b0}
