    \entry{shimanoHiggsModeSuperconductors2020}{article}{}
      \name{author}{2}{}{%
        {{hash=17ff1c494acd6f3b903c338ac915e6f8}{%
           family={Shimano},
           familyi={S\bibinitperiod},
           given={Ryo},
           giveni={R\bibinitperiod}}}%
        {{hash=22e1226cd2a67cedd3a91b1c94b2e830}{%
           family={Tsuji},
