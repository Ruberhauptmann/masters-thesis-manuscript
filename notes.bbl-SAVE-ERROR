    \entry{hazraBoundsSuperconductingTransition2019}{article}{}
      \name{author}{3}{}{%
        {{hash=d33d27d642b1dd95df7dae211aaa0625}{%
           family={Hazra},
           familyi={H\bibinitperiod},
           given={Tamaghna},
