    \entry{hebelNuclearSpinRelaxation1959}{article}{}
      \name{author}{2}{}{%
        {{hash=2bb977a3d901820dcc91157d877a8a46}{%
           family={Hebel},
           familyi={H\bibinitperiod},
           given={L.\bibnamedelimi C.},
           giveni={L\bibinitperiod\bibinitdelim C\bibinitperiod}}}%
        {{hash=0865e78201ed128291d417eac413b952}{%
           family={Slichter},
           familyi={S\bibinitperiod},
           given={C.\bibnamedelimi P.},
           giveni={C\bibinitperiod\bibinitdelim P\bibinitperiod}}}%
