      \verb /home/tjark/Zotero/storage/NJ7RNPN3/Chan et al. - 2022 - Pairing and superconductivity in quasi-one-dimensional flat-band systems Creutz and sawtooth lattic.pdf;/home/tjark/Zotero/storage/3BDGCTX2/PhysRevB.105.html
      \endverb
      \verb{urlraw}
      \verb https://link.aps.org/doi/10.1103/PhysRevB.105.024502
      \endverb
