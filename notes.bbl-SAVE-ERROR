      \strng{bibnamehash}{9ffc0c0f25974a8f82b65890e8b265dd}
      \strng{authorbibnamehash}{9ffc0c0f25974a8f82b65890e8b265dd}
      \strng{authornamehash}{9ffc0c0f25974a8f82b65890e8b265dd}
