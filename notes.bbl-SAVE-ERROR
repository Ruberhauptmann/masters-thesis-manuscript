      \list{publisher}{1}{%
        {Annual Reviews}%
      }
      \strng{namehash}{12421c370d3ea6b67f6f1ec164bb415f}
      \strng{fullhash}{12421c370d3ea6b67f6f1ec164bb415f}
      \strng{bibnamehash}{12421c370d3ea6b67f6f1ec164bb415f}
      \strng{authorbibnamehash}{12421c370d3ea6b67f6f1ec164bb415f}
      \strng{authornamehash}{12421c370d3ea6b67f6f1ec164bb415f}
