    \entry{rybickiPerspectivePhaseDiagram2016}{article}{}
      \name{author}{5}{}{%
        {{hash=c04576aa13d138a607d6c0ab02524664}{%
           family={Rybicki},
           familyi={R\bibinitperiod},
           given={Damian},
           giveni={D\bibinitperiod}}}%
        {{hash=bf6e1183b4fbe40e1e311cb3f09b27f8}{%
