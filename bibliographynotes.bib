  file = {/Users/tjark/Zotero/storage/B2PS35MW/Yanase et al_2003_Theory of superconductivity in strongly correlated electron systems.pdf;/Users/tjark/Zotero/storage/FBA38CZB/S0370157303003235.html}
}

