    \entry{emeryImportancePhaseFluctuations1995}{article}{}
      \name{author}{2}{}{%
        {{hash=4bfbc43962581712da7aad99b0a662f5}{%
           family={Emery},
           familyi={E\bibinitperiod},
           given={V.\bibnamedelimi J.},
           giveni={V\bibinitperiod\bibinitdelim J\bibinitperiod}}}%
        {{hash=651de08fd7cd4cc8d4332fc91d2b324e}{%
           family={Kivelson},
           familyi={K\bibinitperiod},
           given={S.\bibnamedelimi A.},
           giveni={S\bibinitperiod\bibinitdelim A\bibinitperiod}}}%
