      \strng{namehash}{e6cdd0cc037f790a2294b3da3bce36d8}
      \strng{fullhash}{e6cdd0cc037f790a2294b3da3bce36d8}
      \strng{bibnamehash}{e6cdd0cc037f790a2294b3da3bce36d8}
      \strng{authorbibnamehash}{e6cdd0cc037f790a2294b3da3bce36d8}
      \strng{authornamehash}{e6cdd0cc037f790a2294b3da3bce36d8}
