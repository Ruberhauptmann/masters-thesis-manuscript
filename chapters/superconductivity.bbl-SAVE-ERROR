    \entry{provostRiemannianStructureManifolds1980}{article}{}
      \name{author}{2}{}{%
        {{hash=5351c126413483d91097f2b5a555fcd7}{%
           family={Provost},
           familyi={P\bibinitperiod},
           given={J.\bibnamedelimi P.},
           giveni={J\bibinitperiod\bibinitdelim P\bibinitperiod}}}%
        {{hash=dd77d7665c02e3ed8f3262af4ae4b5f3}{%
